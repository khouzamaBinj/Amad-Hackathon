{
  "manifest_version": 3,
  "name": "HoneyWall",
  "version": "2.0",
  "description": "Detect and report phishing URLs",
  "permissions": ["activeTab", "storage", "scripting"],
  "background": {
    "service_worker": "scanner.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  }
}